{
  "name": "appletrailerviewer",
  "version": "1.1.0-alpha",
  "description": "Apple Trailer Viewer is a HTML5/JS sample application that I built to teach myself how to use the latest and greatest tools for modern web application development.",
  "main": "js/app.js",
  "scripts": {
    "test": "echo \"Error: no test specified\" && exit 1",
    "build": "webpack --env production",
    "start": "nodemon --exec \"webpack-dev-server --env development\"",
    "stats": "webpack --env production --profile --json > stats.json"
  },
  "nodemonConfig": {
    "ignore": [
      ".git",
      "node_modules/**/node_modules"
    ],
    "verbose": true,
    "watch": [
      "webpack.*.js"
    ],
    "ext": "js json"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/andrefuller/AppleTrailerViewer.git"
  },
  "keywords": [
    "app",
    "demo",
    "javascript",
    "ui"
  ],
  "author": "Andre Fuller",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/andrefuller/AppleTrailerViewer/issues"
  },
  "homepage": "https://github.com/andrefuller/AppleTrailerViewer#readme",
  "dependencies": {
    "backbone": "^1.3.3",
    "backbone.marionette": "^3.4.2",
    "backbone.paginator": "^2.0.6",
    "bootstrap": "^4.0.0-beta",
    "bootstrap-sass": "^3.3.7",
    "font-awesome": "^4.7.0",
    "google-fonts-webpack-plugin": "^0.4.2",
    "jquery": "^3.2.1",
    "moment": "^2.18.1",
